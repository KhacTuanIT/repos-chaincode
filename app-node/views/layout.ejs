  <script>
    $(document).on('keyup', '#search-input', function (e) {
      if (e.keyCode == 13) {
        e.preventDefault();
        let userId = e.target.value;
        if (userId.length > 0) {
          $.ajax({
            url: `/api/users/${userId}`,
            type: 'get',
            success: function (res) {
              let { user } = res;
              $('#user-id').text(user.userId);
              $('#user-fn').text(user.firstName);
              $('#user-mn').text(user.middleName);
              $('#user-ln').text(user.lastName);
              $('#user-email').text(user.email);
              $('#user-address').text(user.address);
              $('#userDetailSearch').modal('show');
              $('#search-input').val("");
            },
            error: function (jq, status, err) {
              console.log(jq);
            }
          })
        }
      }
    })
  </script>
