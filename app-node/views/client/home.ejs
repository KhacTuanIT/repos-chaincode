<!-- Begin Li's Laptop Product Area -->
<section class="product-area li-laptop-product pt-60 pb-45">
  <div class="container">
    <div class="row">
      <!-- Begin Li's Section Area -->
      <div class="col-lg-12">
        <div class="li-section-title">
          <h2>
            <span>Laptop</span>
          </h2>
        </div>
        <div class="row">
          <div class="product-active owl-carousel">
            <% if (laptops) { %> <% for (var index = 0; index < laptops.length;
            index++) { %>
            <div class="col-lg-12">
              <!-- single-product-wrap start -->
              <div class="single-product-wrap">
                <div class="product-image">
                  <a href="/product-details/<%= laptops[index].Value.code %>">
                    <img
                      src="/images/<%= laptops[index].Value.primaryImage %>"
                      alt="<%= laptops[index].Value.name %>"
                    />
                  </a>
                  <span class="sticker">New</span>
                </div>
                <div class="product_desc">
                  <div class="product_desc_info">
                    <div class="product-review">
                      <h5 class="manufacturer">
                        <a href="shop-left-sidebar.html"
                          ><%= laptops[index].Value.manufacturer.Value.name
                          %></a
                        >
                      </h5>
                    </div>
                    <h4>
                      <a
                        class="product_name"
                        href="/product-details/<%= laptops[index].Value.code %>"
                        ><%= laptops[index].Value.name %></a
                      >
                    </h4>
                    <div class="price-box">
                      <span class="new-price"
                        >$<%= laptops[index].Value.price %></span
                      >
                    </div>
                  </div>
                  <div class="add-actions">
                    <ul class="add-actions-link">
                      <li class="add-cart active">
                        <a href="#">Add to cart</a>
                      </li>
                      <li>
                        <a
                          href="#"
                          title="quick view"
                          class="quick-view-btn"
                          data-toggle="modal"
                          data-target="#exampleModalCenter"
                          ><i class="fa fa-eye"></i
                        ></a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
              <!-- single-product-wrap end -->
            </div>
            <% } %> <% } %>
          </div>
        </div>
      </div>
      <!-- Li's Section Area End Here -->
    </div>
  </div>
</section>
<!-- Li's Laptop Product Area End Here -->
